namespace LALR {
namespace DPDA {

class ReadTransition : LALR.DPDA.Transition {
    var symbol;
    function symbol(symbol[optional], int has_symbol[opt_flag]) {
	if (has_symbol) {
	    self.symbol = symbol;
	}
	return self.symbol;
    }

    function is_hash_transition() {
	return substr(self.symbol, 0, length("__CFSM_HASH")) == "__CFSM_HASH";
    }

}

}
}